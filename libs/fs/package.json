{
  "name": "@puq/fs",
  "version": "0.11.12",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "File sytem",
  "homepage": "https://beemood.github.io/libs/fs",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://beemood.github.io"
  },
  "icon": "https://beemood.github.io/libs/fs/assets/favicon.png",
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "keywords": [
    "fs"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@puq/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/fs/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/fs/dist",
          "main": "libs/fs/src/index.ts",
          "tsConfig": "libs/fs/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@puq/testing": "workspace:*",
    "@types/js-yaml": "^4.0.9"
  }
}
