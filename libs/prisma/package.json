{
  "name": "@puq/prisma",
  "version": "0.16.3",
  "license": "MIT",
  "description": "The project provides a nestjs module that flawlessly integrates the project with prisma orm by providing repositories, database client, providers, and injectrors.",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "homepage": "https://beemood.github.io/puq/libs/prisma",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://beemood.github.io"
  },
  "icon": "https://beemood.github.io/puq/libs/prisma/assets/favicon.png",
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "keywords": [],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/prisma/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/prisma/dist",
          "main": "libs/prisma/src/index.ts",
          "tsConfig": "libs/prisma/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@prisma/client": "6.16.2",
    "express": "^5.1.0"
  },
  "dependencies": {
    "@puq/names": "workspace:*",
    "@puq/types": "workspace:*",
    "@puq/utils": "workspace:*",
    "@swc/helpers": "~0.5.11"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3"
  }
}