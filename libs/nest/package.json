{
  "name": "@puq/nest",
  "version": "0.20.0",
  "license": "MIT",
  "description": "The project is a wrapper around nestjs decorators. It provides combined decorators to keep code base clean and consistent. The project follows the common standard implementaions pattern of nestjs, swagger, zod, and prisma orm.",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "homepage": "https://beemood.github.io/libs/nest",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://beemood.github.io"
  },
  "icon": "https://beemood.github.io/libs/nest/assets/favicon.png",
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "keywords": [],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/nest/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/nest/dist",
          "main": "libs/nest/src/index.ts",
          "tsConfig": "libs/nest/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "peerDependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "cache-manager": "^7.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "rxjs": "^7.8.2",
    "zod": "^4.1.11",
    "@prisma/client": "6.16.2"
  },
  "dependencies": {
    "@puq/errors": "workspace:*",
    "@puq/names": "workspace:*",
    "@puq/types": "workspace:*",
    "@puq/utils": "workspace:*",
    "@swc/helpers": "~0.5.11",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0"
  }
}
