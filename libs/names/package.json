{
  "name": "@puq/names",
  "version": "0.25.1",
  "license": "MIT",
  "displayName": "Names",
  "description": "The library provides a useful function to create all casing variants of a given string value such as title-case, pascal-case, snake-case and so more.",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "homepage": "https://beemood.github.io/libs/names",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://beemood.github.io"
  },
  "icon": "https://beemood.github.io/libs/names/assets/favicon.png",
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@puq/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/names/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/names/dist",
          "main": "libs/names/src/index.ts",
          "tsConfig": "libs/names/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@puq/errors": "workspace:*",
    "@puq/types": "workspace:*",
    "@puq/utils": "workspace:*",
    "@swc/helpers": "~0.5.11"
  }
}
