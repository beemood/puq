{
  "name": "@puq/zod",
  "version": "0.29.4",
  "license": "MIT",
  "displayName": "Common Zod Schemas",
  "description": "The project provides common zod schemas and utilities.",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "homepage": "https://beemood.github.io/puq/libs/zod",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://beemood.github.io"
  },
  "icon": "https://beemood.github.io/puq/libs/zod/assets/favicon.png",
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "keywords": [],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/zod/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/zod/dist",
          "main": "libs/zod/src/index.ts",
          "tsConfig": "libs/zod/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx typedoc"
          ],
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "peerDependencies": {
    "zod": "^4.1.11"
  },
  "dependencies": {
    "@puq/types": "workspace:*",
    "@swc/helpers": "~0.5.11"
  }
}
